    "next": "16.1.4",
    "next-themes": "0.4.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tailwind-merge": "3.4.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "4.1.18",
    "@types/mdx": "2.0.13",
    "@types/node": "25.0.10",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
